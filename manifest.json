{
  "name": "ytsbar_sticky",
  "version": "1.0.0",
  "description": "",
  "manifest_version": 2,
  "content_scripts": [{
    "matches": [
      "https://www.youtube.com/",
      "https://www.youtube.com/*"
    ],
    "js": ["main.js"]
  }],
  "permissions": ["storage", "activeTab"],
  "icons": {
    "48": "/images/icon48.png",
    "128": "/images/icon128.png"
  }
}
